{
    "manifest_version": 2,
    "name": "Expense Tracker",
    "description": "Ext that tracks users expenses",
    "version": "0.1",
    "icons": {
        "128": "Cash-icon-128.png",
        "48": "Cash-icon-48.png",
        "16": "Cash-icon-16.png"
    },
    "browser_action": {
        "default_icon": "Cash-icon-16.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    
    "permissions": [
        "storage"
        
    ]
}